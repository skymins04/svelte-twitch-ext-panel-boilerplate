{
  "arrowParens": "avoid",
  "singleQuote": false,
  "printWidth": 90,
  "plugins": ["prettier-plugin-svelte"],
  "semi": true,
  "svelteSortOrder": "options-styles-scripts-markup",
  "svelteStrictMode": true,
  "svelteBracketNewLine": false,
  "svelteIndentScriptAndStyle": true,
  "trailingComma": "all"
}